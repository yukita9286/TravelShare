<table class='table'>
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% customers.each do |customer| %>
      <tr>
        <td><%= image_tag customer.get_profile_image(100,100) %></td>
        <td><%= customer.name %></td>
        <td><%= render "relationships/btn", customer: customer %></td>
        <td><%#= link_to 'Show', user, class: "customer_#{customer.id}" %></td>
      </tr>
    <% end %>
  </tbody>
</table>