<h2>Users</h2>
<table class="table">

 <thead>
  <tr>

      <th>image</th>
      <th>name</th>
      <th colspan="3"></th>

  </tr>
 </thead>

  <tbody>
   <% @customers.each do |customer| %>
    
    <tr>
     
    <div><td><%= image_tag customer.get_profile_image(100,100) %></td></div>
      <td><%= customer.name %></td>
      <td><%= link_to 'Show', admin_customer_path(customer.id) %></td>

    </tr>
    
   <% end %>
  </tbody>
</table>